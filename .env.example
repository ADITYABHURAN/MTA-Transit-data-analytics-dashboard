# Environment variables for MTA Transit Dashboard
# Copy this file to .env and update with your values

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=mta_transit_db
DB_USER=postgres
DB_PASSWORD=your_password_here

# NYC Open Data API Token (optional, for higher rate limits)
# Get your token at: https://data.ny.gov/profile/edit/developer_settings
NYC_OPEN_DATA_TOKEN=

# Logging Level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO
